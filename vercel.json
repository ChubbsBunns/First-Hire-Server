{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "/"
     }
    ],
    "crons": [
        {
            "path": "/getDailyData",
            "schedule": "35 15 * * *"
        }
    ]
   }